{% extends "KhatovarWebBundle::layout.html.twig" %}

{% block inarticle %}
    <h2>Ajouter une photo</h2>

    <div class="form-background">
        {{ form_start(form) }}
        {{ form_errors(form) }}

        <div class="form-group">
            {{ form_row(form.file) }}
        </div>

        {% embed '@KhatovarWeb/Photo/upload.html.twig' %}{% endembed %}

        {{ form_end(form) }}
    </div>
{% endblock %}
